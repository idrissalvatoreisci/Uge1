


color green = #008000;
color yellow = #ffff00; 
color red = #ff0000; 

void setup() {
  size(500,500);
  background(255);
  
  rect(100,0,200,400);
  strokeWeight(2);
  stroke(20);


}

void draw() {


  ellipse(200,100,100,100);
  fill(red);

  ellipse(200,200,100,100);
  fill(yellow);

  ellipse(200,300,100,100);
  fill(green);
  

}
